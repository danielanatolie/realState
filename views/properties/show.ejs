<% include ../partials/header %>
<h1><%= property.name %></h1>
<img src="<%= property.image %>">
<p><%= property.description %></p>
<% property.comments.forEach(function(comment) { %>
    <p>
        <strong><%= comment.author %></strong> - <%= comment.text %>
    </p>
<% }) %>
<% include ../partials/footer %>